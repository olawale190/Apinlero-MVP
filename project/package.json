{
  "name": "apinlero-mvp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "start": "node server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check-env": "node scripts/check-env.js",
    "validate-stripe": "node scripts/validate-stripe-keys.js",
    "pre-deploy-check": "node scripts/pre-deploy-check.js",
    "deploy": "npm run pre-deploy-check && vercel --prod",
    "deploy:force": "vercel --prod",
    "security:setup": "bash scripts/security/setup-all.sh",
    "security:layer1": "bash scripts/security/setup-layer1.sh",
    "security:layer2": "bash scripts/security/setup-layer2.sh",
    "security:layer3": "bash scripts/security/setup-layer3.sh",
    "security:layer4": "bash scripts/security/setup-layer4.sh",
    "security:sbom": "syft dir:. -o json > sbom.json",
    "security:scan": "grype dir:. --fail-on high -o table",
    "security:scan:sbom": "grype sbom:sbom.json --only-fixed --fail-on high -o table",
    "security:snyk": "snyk test && snyk code test",
    "security:monitor": "snyk monitor"
  },
  "dependencies": {
    "@react-email/components": "^1.0.4",
    "@react-email/render": "^2.0.2",
    "@sentry/react": "^10.37.0",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@supabase/supabase-js": "^2.93.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "lucide-react": "^0.344.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.7.0",
    "resend": "^6.7.0",
    "serve": "^14.2.1",
    "twilio": "^5.11.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^27.4.0",
    "pg": "^8.17.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vercel": "^50.9.6",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "tar": "^7.5.7",
    "undici": "^7.3.0",
    "semver": "^7.7.2",
    "debug": "^4.4.1",
    "path-to-regexp": "^8.2.0",
    "esbuild": "^0.25.2"
  }
}
